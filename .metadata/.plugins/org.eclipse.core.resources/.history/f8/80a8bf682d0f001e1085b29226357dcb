package com.example.demo.phone;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

//메타 정보를 작성할 설정파일 
@Configuration // 선언하고 시작

public class PhoneConfig {
	//빈을 만들어보자
	@Bean
	public Iphone iphone14() {
		return new Iphone();
	}
	
	@Bean
	public GalaxyFold galaxy() {
		return new GalaxyFold();
	}
	@Bean
	public PhoneUser phoneUser() { //의존성 주입
		PhoneUser user = new PhoneUser(galaxy());
		return user;
	}
	}
